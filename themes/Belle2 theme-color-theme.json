{
  "name": "Belle II Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#222831",
    "editor.foreground": "#EEEEEE",
    "editorCursor.foreground": "#FFD700",
    "editor.lineHighlightBackground": "#8b7f0069",
    "editor.selectionBackground": "#DAA520",
    "editorLineNumber.foreground": "#FFD700",
    "editorLineNumber.activeForeground": "#EEEEEE",
    "activityBar.background": "#0033A0",
    "sideBar.background": "#222831",
    "sideBar.foreground": "#EEEEEE",
    "statusBar.background": "#0033A0",
    "statusBar.foreground": "#EEEEEE",
    "titleBar.activeBackground": "#0033A0",
    "titleBar.activeForeground": "#EEEEEE",
    "tab.activeBackground": "#0033A0",
    "tab.inactiveBackground": "#222831",
    "tab.activeForeground": "#EEEEEE",
    "tab.inactiveForeground": "#FFD700",
    "terminal.background": "#222831",
    "terminal.foreground": "#EEEEEE"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#708090"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#00ADB5"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#F3E5AB"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#FFC000"
      }
    },
    {
      "scope": "function, method",
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": "type",
      "settings": {
        "foreground": "#0033A0"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#EEEEEE"
      }
    },
	{
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#dacde4" // Example color for classes
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#dacde4" // Example color for functions
      }
    },
	{
	"scope": ["source"],
		"settings": {
			"foreground": "#bfeaec", 
			}
	},
	{
	"scope": ["support.function.builtin"],
	"settings": {
		"foreground": "#00ADB5"
		}
	},
	{
	"scope": ["meta.member.access"],
	"settings": {
		"foreground": "#0080b5"
		}
	},
	{
	"scope": ["meta.function-call"],
	"settings": {
		"foreground": "#A6B1E1"
		}
	},
  ]
}